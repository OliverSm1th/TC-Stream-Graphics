{
  "dependencies": {
    "gsap": "^3.12.7",
    "inliner": "^1.13.1",
    "mongodb": "^6.13.0"
  },
  "scripts": {
    "build": "find ${npm_config_file} -maxdepth 1 -name '*.html' | inliner > build/${npm_config_file}.html",
    "server": "http-server -p 8080 -c-1",
    "sass": "sass --watch .",
    "tail": "file2=$(find ${npm_config_file} -maxdepth 1 -name '*.css' | sed 's/\\.css$//1'); npx @tailwindcss/cli -i ${file2}.css -o ${file2}-build.css --watch"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "parcel": "^2.13.3",
    "sass": "^1.84.0",
    "tailwind": "^4.0.0"
  }
}
